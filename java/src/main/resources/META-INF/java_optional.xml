<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <!-- Java -> proto goto -->
    <gotoDeclarationHandler
      implementation="idea.plugin.protoeditor.java.PbJavaGotoDeclarationHandler"/>
    <fileBasedIndex
      implementation="idea.plugin.protoeditor.java.PbJavaOuterClassIndex"/>
    <!-- proto -> java find usages -->
    <findUsagesHandlerFactory
      implementation="idea.plugin.protoeditor.java.PbJavaFindUsagesHandlerFactory"/>
  </extensions>
</idea-plugin>
